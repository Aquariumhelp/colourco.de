{
  "name": "colourcode",
  "version": "1.0.0-alpha.1",
  "description": "Pick colours based on hsl. Scheme-mode with monochromatic, analogic, complement, triad and quad colours",
  "main": "dist/index.js",
  "private": true,
  "config": {
    "awsBucketName": "colourco.de",
    "awsRegion": "eu-central-1",
    "awsProfile": "nextcode"
  },
  "scripts": {
    "build": "webpack",
    "build:production": "npm run build --production",
    "start": "webpack-dev-server --hot --inline --open -d",
    "watch": "npm run build -- --watch",
    "test": "echo \"Error: no test specified\" && exit 1",
    "create-bucket": "aws s3 mb s3://$npm_package_config_awsBucketName --region $npm_package_config_awsRegion --profile $npm_package_config_awsProfile",
    "delete-bucket": "aws s3 rb s3://$npm_package_config_awsBucketName --region $npm_package_config_awsRegion --force --profile $npm_package_config_awsProfile",
    "upload": "aws s3 cp ./dist s3://$npm_package_config_awsBucketName --recursive --acl public-read --region $npm_package_config_awsRegion --profile $npm_package_config_awsProfile",
    "deploy": "npm run build && npm run upload && rm -rf dist"
  },
  "keywords": [
    "colour",
    "color",
    "colourcode",
    "colorcode",
    "swatch",
    "scheme",
    "ts",
    "tslint",
    "tsx",
    "typescript",
    "vue",
    "vuejs",
    "webpack",
    "webpack4"
  ],
  "author": {
    "name": "Tamino Martinius",
    "url": "https://github.com/tamino-martinius/"
  },
  "license": "MIT",
  "dependencies": {
    "vue": "^2.5.16",
    "vue-class-component": "^6.2.0",
    "vue-property-decorator": "^6.1.0",
    "vue-router": "^3.0.1"
  },
  "devDependencies": {
    "clean-webpack-plugin": "^0.1.19",
    "copy-webpack-plugin": "^4.5.1",
    "css-loader": "^0.28.11",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "node-sass": "^4.9.0",
    "sass-loader": "^7.0.3",
    "ts-loader": "^4.4.1",
    "tslint": "^5.10.0",
    "tslint-config-airbnb": "^5.9.2",
    "typescript": "^2.9.2",
    "vue-loader": "^15.2.4",
    "vue-svg-loader": "^0.5.0",
    "vue-template-compiler": "^2.5.16",
    "webpack": "^4.12.0",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.4"
  }
}
